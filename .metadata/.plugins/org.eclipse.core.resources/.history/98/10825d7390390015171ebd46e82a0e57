int myAtoi(char* str){
	int flags = 1;
	int res = 0;
	if(!str) return 0;
	if(*str == '+') str++;
	if(*str == '-') {flags = -1;str++;}
	while(*str>='0'&&*str<='9'){
		res = res*10+*str-'0';
	}
	return res*flags;
}

